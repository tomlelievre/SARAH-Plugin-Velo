<grammar version="1.0" xml:lang="fr-FR" mode="voice" root="ruleVelov" xmlns="http://www.w3.org/2001/06/grammar"
         tag-format="semantics/1.0">
    <rule id="ruleVelov" scope="public">
        <example>Sarah .</example>
        <tag>out.action=new Object();</tag>

        <item>Sarah</item>

        <one-of>
            <item>remet le lance missile en position initiale
                <tag>out.action.seat="park";</tag>
            </item>
            <item>tire <ruleref special="GARBAGE" /> foi à l'entrée
                <tag>out.action.seat="entry";</tag>
            </item>
        </one-of>
        <tag>out.action._attributes.uri="http://127.0.0.1:8081/sarah/cheekyto";</tag>
        <tag>out.action._attributes.dictation="true";</tag>
    </rule>
</grammar>
